<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=h1, initial-scale=1.0">
    <title>Aula 02 - NodeJS (Atividade)</title>
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</head>
<body class="grey lighten-2">
    <nav class="grey darken-4">
        <div class="nav-wrapper container">
            <!-- Comunidade Coders Ceará -->
            <a href="/" class="brand-logo">Navegano</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
              <li><a href="/cardapio">Cardapio</a></li>
              <li><a href="/criar">Criar</a></li>
              <li><a href="#">Dúvidas</a></li>
            </ul>
        </div>
    </nav>
    <section class="container">
        <h3>Cardapio:</h3>
        
        <h4>Pratos veganos:</h4>

        <% rows.forEach((item) => {%>
          <div class="row">
            <div class="col s12 m7">
              <div class="card">
                <div class="card-image">
                  <img src="<%= item.URL%>">
                </div>
                <div class="card-content">
                  <span class="card-title"><b><%= item.Title %></b></span>
                  <p><%= item.Descricao %></p>
                  <!-- <h6><%= item.data %></h6> -->
                  <h4><%= item.Price%></h4>
                </div>
                <div class="card-action">
                  <a class="grey-darken-4" href="/delete/<%= item.Title %>">Deletar</a>
                  <a class="grey-darken-4" href="/editar/<%= item.Title %>">Editar</a>

                </div>
              </div>
            </div>
          </div>
          <% }); %>
    </section>
</body>
</html>